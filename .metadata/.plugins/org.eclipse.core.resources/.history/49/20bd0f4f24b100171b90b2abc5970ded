package ancestors;

public class Tree {

	public Node root;
	
	public Tree(int rootValue){
		root = new Node(null, rootValue);
	}
	
	public void addLeft(int value){
		root.addLeft(value);
	}
	
	public void addRight(int value){
		root.addRight(value);
	}
	
	public Node left(){
		return root.left;
	}
	
	public Node right(){
		return root.right;
	}
	
	public Node getLowestCommonAncestor(Node a, Node b){
		return null;
	}
	
}
